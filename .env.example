# Base API Configuration
# Copy this file to .env.local and fill in your actual values

# Base API domain
BASE_DOMAIN=https://account.base.vn

# Refresh token for authentication
BASE_REFRESH_TOKEN=your_refresh_token_here

# Session cookie (optional)
BASE_COOKIE=basessid=your_cookie_here

# Access token for API requests (obtained from refreshToken)
BASE_ACCESS_TOKEN=your_access_token_here
